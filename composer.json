{
    "name": "pragmarx/zipcode",

    "description": "A worldwide address-by-zipcode searcher.",

    "keywords": ["address", "zip code", "zip", "laravel"],

    "license": "BSD-3-Clause",

    "authors": [
        {
            "name": "Antonio Carlos Ribeiro",
            "email": "acr@antoniocarlosribeiro.com",
            "role": "Creator"
        }
    ],

    "require": {
        "pragmarx/support": "0.9.0",
        "guzzlehttp/guzzle": "~4|~5|~6",
        "illuminate/filesystem": "~4|~5|~6",
        "laravel/helpers": "^1.1"
    },

    "require-dev": {
        "phpspec/phpspec": "2.0.*@dev"
    },

    "autoload": {
        "psr-4": {
            "PragmaRX\\ZipCode\\": "src/"
        }
    },

    "extra": {
        "component": "package",
        "branch-alias": {
            "dev-master": "1.6.x-dev"
        },
        "laravel": {
            "providers": [
                "PragmaRX\\ZipCode\\Vendor\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "ZipCode" : "PragmaRX\\ZipCode\\Vendor\\Laravel\\Facade"
            }
        }
    },

    "minimum-stability": "dev"
}
